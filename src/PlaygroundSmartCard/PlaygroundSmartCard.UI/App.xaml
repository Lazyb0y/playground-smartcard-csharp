<Application x:Class="PlaygroundSmartCard.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PlaygroundSmartCard.UI"
             xmlns:converters="clr-namespace:PlaygroundSmartCard.UI.Converters"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converters:InverseNullToBoolConverter x:Key="InverseNullToBoolConverter" />
            <converters:InverseStringToBoolConverter x:Key="InverseStringToBoolConverter" />
            <converters:NullToBoolConverter x:Key="NullToBoolConverter" />
            <converters:StringToBoolConverter x:Key="StringToBoolConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>